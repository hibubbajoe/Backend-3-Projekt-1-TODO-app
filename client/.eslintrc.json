{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": 0,
        "react/jsx-filename-extension": 0,
        "max-len": 0,
        "react/prop-types": 0,
        "no-undef": 0,
        "react/react-in-jsx-scope": 0,
        "react/destructuring-assignment": 0,
        "no-console": 0
    }
}
